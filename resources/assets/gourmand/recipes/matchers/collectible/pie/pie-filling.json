[
  {
    categories: [
      {
        input: "cookable-food",
        outputs: [ "pie-filling-code" ]
      },
      {
        input: "cookable-food-category",
        outputs: [ "pie-filling-category" ]
      },
    ],
    attributes: [
      {
        path: ["inPieProperties", "partType"],
        value: "Filling"
      }
    ]
  },
  {
    // Exclude any foods from pies that have no nutrition when in meals. These
    // foods would cause a crash when rendered. See
    // https://github.com/bluelightning32/vs-gourmand/issues/2.
    priority: 2,
    categories: [
      {
        input: "cookable-no-nutrition",
      },
    ],
    deletes: [ "pie-filling-code", "pie-filling-category" ]
  }
]
