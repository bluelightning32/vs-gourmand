  {
    // A match nothing rule to register the meal-topping-sauce category, in case expandedfoods is not installed.
    deletes: [ "meal-scrambledeggs-milk-base" ]
  },
  {
    dependsOn: [ "expandedfoods" ],
    cooking : {
      recipe: "scrambledeggs",
      ingredient: "milk-base",
      outputs: ["meal-scrambledeggs-milk-base"]
    }
  }
]
